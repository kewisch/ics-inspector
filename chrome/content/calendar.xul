<!DOCTYPE overlay SYSTEM "chrome://ics-inspector/locale/inspector.dtd">

<overlay id="calendar-list-overlay"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/x-javascript" src="chrome://ics-inspector/content/inspector.js"/>

  <popup id="context-menu">
    <menuitem id="ics-inspector-inspect-event"
              label="&ics-inspector.context.inspect.label;"
              insertafter="general-context-menu-delete"
              oncommand="gICSInspector.inspectEvent()"/>
  </popup>

  <popup id="calendar-view-context-menu">
    <menuitem id="ics-inspector-view-inspect-event"
              label="&ics-inspector.context.inspect.label;"
              oncommand="gICSInspector.inspectEvent()"/>
  </popup>

  <popup id="calendar-item-context-menu">
    <menuitem id="ics-inspector-item-inspect-event"
              label="&ics-inspector.context.inspect.label;"
              oncommand="gICSInspector.inspectEvent()"/>
  </popup>

  <vbox id="agenda-menu-box">
    <menuitem id="ics-inspector-item-inspect-event"
              label="&ics-inspector.context.inspect.label;"
              oncommand="gICSInspector.agendaInspectEvent(event)"/>
  </vbox>

  <menupopup id="taskitem-context-menu">
    <menuitem id="ics-inspector-item-inspect-task"
              label="&ics-inspector.context.inspect.label;"
              insertafter="task-context-menu-delete"
              oncommand="gICSInspector.inspectTask()"/>
  </menupopup>

  <popup id="list-calendars-context-menu">
    <menuitem id="ics-inspector-inspect-calendar"
              label="&ics-inspector.context.inspect.label;"
              insertafter="list-calendars-context-publish"
              oncommand="gICSInspector.inspectCalendar()"/>
  </popup>
</overlay>
